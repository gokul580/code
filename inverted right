#include <stdio.h>

int main() {
    int n;
    printf("enter no:");
    scanf("%d",&n);
    
    for(int i=n;i<=n;i--)
    {
        for(int s=1;s<=n;s++)
        {
            printf(" ");
        }

        for(int j=1;j<=i;j++)
        {
            printf("*");
        }
        
        printf("\n");
    }
    return 0;
}
