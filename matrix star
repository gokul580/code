#include <stdio.h>

int main() {
    int n,r,c;
    printf("enter no of row");
    scanf("%d",&r);
    printf("enter no of column");
    scanf("%d",&c);
    for(int i=1;i<=r;i++)
    {
        for(int j=1;j<=c;j++)
        {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}
